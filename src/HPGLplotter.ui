                    <property name="margin-start">4</property>
                    <property name="valign">end</property>
                    <signal name="clicked" handler="CB_btn_OK"/>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
